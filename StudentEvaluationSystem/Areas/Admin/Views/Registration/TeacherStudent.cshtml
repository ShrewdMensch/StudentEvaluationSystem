@model StudentEvaluationSystem.Models.ViewModels.TeacherStudentRegistrationViewModel

@{
    ViewData["Title"] = "TeacherStudent";
}

<h2>TeacherStudent</h2>

<div class="site-section">
    <section id="tabs">
        <div class="container">
            <div class="row">
                <div class="col-md-12 ">
                    <nav>
                        <div class="nav nav-tabs nav-fill" id="nav-tab" role="tablist">
                            <a class="nav-item nav-link active" id="nav-student-tab" data-toggle="tab" href="#nav-student" role="tab" aria-controls="nav-student" aria-selected="true">Student</a>
                            <a class="nav-item nav-link" id="nav-teacher-tab" data-toggle="tab" href="#nav-teacher" role="tab" aria-controls="nav-teacher" aria-selected="false">Teacher</a>
                        </div>
                    </nav>
                    <div class="tab-content" id="nav-tabContent">
                        <div class="tab-pane fade show active" id="nav-student" role="tabpanel" aria-labelledby="nav-student-tab">
                            <partial name="~/Views/Partial/_StudentRegistrationForm.cshtml" model="@Model.StudentRegistrationViewModel" />
                        </div>
                        <div class="tab-pane fade" id="nav-teacher" role="tabpanel" aria-labelledby="nav-teacher-tab">
                            <partial name="~/Views/Partial/_TeacherRegistrationForm.cshtml" model="@Model.TeacherRegistartion" />
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </section>
    <!-- ./Tabs -->
</div>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
    <script>
        $(function () {
            $("#datePicker").datepicker();

        });
    </script>
}